{
  "name": "@outspeed/react-native",
  "version": "1.0.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./@types": {
      "types": "./dist/@types.d.ts",
      "import": "./dist/@types.mjs",
      "require": "./dist/@types.js"
    },
    "./RealtimePlayer": {
      "types": "./dist/components/RealtimePlayer.d.ts",
      "import": "./dist/components/RealtimePlayer.mjs",
      "require": "./dist/components/RealtimePlayer.js"
    },
    "./isAValidRTCSessionDescription": {
      "types": "./dist/connection/isAValidRTCSessionDescription.d.ts",
      "import": "./dist/connection/isAValidRTCSessionDescription.mjs",
      "require": "./dist/connection/isAValidRTCSessionDescription.js"
    },
    "./RealtimeConnection": {
      "types": "./dist/connection/RealtimeConnection.d.ts",
      "import": "./dist/connection/RealtimeConnection.mjs",
      "require": "./dist/connection/RealtimeConnection.js"
    },
    "./RealtimeConnectionMediaManager": {
      "types": "./dist/connection/RealtimeConnectionMediaManager.d.ts",
      "import": "./dist/connection/RealtimeConnectionMediaManager.mjs",
      "require": "./dist/connection/RealtimeConnectionMediaManager.js"
    },
    "./RealtimeConnectionNegotiator": {
      "types": "./dist/connection/RealtimeConnectionNegotiator.d.ts",
      "import": "./dist/connection/RealtimeConnectionNegotiator.mjs",
      "require": "./dist/connection/RealtimeConnectionNegotiator.js"
    },
    "./useAvailableMediaDevices": {
      "types": "./dist/hooks/useAvailableMediaDevices.d.ts",
      "import": "./dist/hooks/useAvailableMediaDevices.mjs",
      "require": "./dist/hooks/useAvailableMediaDevices.js"
    },
    "./useWebRTC": {
      "types": "./dist/hooks/useWebRTC.d.ts",
      "import": "./dist/hooks/useWebRTC.mjs",
      "require": "./dist/hooks/useWebRTC.js"
    },
    "./SDP": {
      "types": "./dist/SDP.d.ts",
      "import": "./dist/SDP.mjs",
      "require": "./dist/SDP.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.js"
    }
  },
  "typesVersions": {
    "*": {
      "@types": [
        "./dist/@types.d.ts"
      ],
      "RealtimePlayer": [
        "./dist/components/RealtimePlayer.d.ts"
      ],
      "isAValidRTCSessionDescription": [
        "./dist/connection/isAValidRTCSessionDescription.d.ts"
      ],
      "RealtimeConnection": [
        "./dist/connection/RealtimeConnection.d.ts"
      ],
      "RealtimeConnectionMediaManager": [
        "./dist/connection/RealtimeConnectionMediaManager.d.ts"
      ],
      "RealtimeConnectionNegotiator": [
        "./dist/connection/RealtimeConnectionNegotiator.d.ts"
      ],
      "useAvailableMediaDevices": [
        "./dist/hooks/useAvailableMediaDevices.d.ts"
      ],
      "useWebRTC": [
        "./dist/hooks/useWebRTC.d.ts"
      ],
      "SDP": [
        "./dist/SDP.d.ts"
      ],
      "utils": [
        "./dist/utils.d.ts"
      ]
    }
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "check-exports": "node ../../scripts/check-exports.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/outspeed-ai/outspeed-js",
    "directory": "packages/react-native"
  },
  "keywords": [],
  "author": "",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.75.3",
    "react-native-incall-manager": "^4.2.0",
    "react-native-webrtc": "^124.0.4"
  },
  "devDependencies": {
    "@types/react": "^18.3.10",
    "tsup": "8.2.4"
  }
}